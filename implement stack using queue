class MyStack {
   
    Deque<Integer> q1;
    public MyStack() {
       q1=new ArrayDeque<Integer>();
    }
    
    public void push(int x) {
        q1.addFirst(x);
    }
    
    public int pop() {
        return q1.poll();
    }
    
    public int top() {
        return q1.peek();
    }
    
    public boolean empty() {
        return q1.size()==0;
    }
}
